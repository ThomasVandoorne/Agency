{% extends "_/layout/base.twig" %}
{% block content %}
    <h1 class="project__title"> {{ entry.title }}</h1>
        {% for social in entry.socialMedia.all() %}
                <div class="socials"> 
                <img class="socials__img" src="{{ social.socialImage.one().url }}" alt="">
                <p>{{social.socialName}}</p>
                </div >
    {% endfor %}
{% endblock %}